    <field name="resultados" type="text" column="RESULTADOS" length="65535" nullable="true"/>
    <field name="observaciones" type="text" column="OBSERVACIONES" length="65535" nullable="true"/>
    <field name="fechaReingreso" type="date" column="FECHA_REINGRESO" nullable="true"/>
    <field name="fechaEgreso" type="date" column="FECHA_EGRESO" nullable="true"/>
    <field name="causaEgreso" type="text" column="CAUSA_EGRESO" length="65535" nullable="true"/>
    <field name="descripcion" type="text" column="DESCRIPCION" length="65535" nullable="true"/>
  </entity>
</doctrine-mapping>
