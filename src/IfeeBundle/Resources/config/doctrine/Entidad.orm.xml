<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IfeeBundle\Entity\Entidad" table="entidad">
    <id name="idEntidad" type="integer" column="ID_ENTIDAD">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="razonSocial" type="text" column="RAZON_SOCIAL" length="65535" nullable="true"/>
    <field name="nombreCorto" type="text" column="NOMBRE_CORTO" length="65535" nullable="true"/>
    <field name="direccion" type="text" column="DIRECCION" length="65535" nullable="true"/>
    <field name="telefono" type="text" column="TELEFONO" length="65535" nullable="true"/>
    <field name="correo" type="text" column="CORREO" length="65535" nullable="true"/>
    <field name="ruc" type="text" column="RUC" length="65535" nullable="true"/>
    <field name="fechaCreacion" type="date" column="FECHA_CREACION" nullable="true"/>
    <field name="descripcion" type="text" column="DESCRIPCION" length="65535" nullable="true"/>
    <field name="rutaLogo" type="text" column="RUTA_LOGO" length="65535" nullable="true"/>
  </entity>
</doctrine-mapping>
