<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IfeeBundle\Entity\DatosPersonal" table="datos_personal">
    <id name="idDatosPersonal" type="integer" column="ID_DATOS_PERSONAL">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombres" type="text" column="NOMBRES" length="65535" nullable="true"/>
    <field name="apellidos" type="text" column="APELLIDOS" length="65535" nullable="true"/>
    <field name="cedula" type="integer" column="CEDULA" nullable="true"/>
    <field name="cargo" type="text" column="CARGO" length="65535" nullable="true"/>
    <field name="tipo" type="text" column="TIPO" length="65535" nullable="true"/>
    <field name="ciclo" type="text" column="CICLO" length="65535" nullable="true"/>
    <field name="estado" type="text" column="ESTADO" length="65535" nullable="true"/>
    <field name="fechaIngresoInstitucion" type="text" column="FECHA_INGRESO_INSTITUCION" length="65535" nullable="true"/>
    <field name="telefono" type="text" column="TELEFONO" length="65535" nullable="true"/>
    <field name="email" type="text" column="EMAIL" length="65535" nullable="true"/>
    <field name="direccion" type="text" column="DIRECCION" length="65535" nullable="true"/>
  </entity>
</doctrine-mapping>
